{
    "version": "4.0.4",
    "description": "Flutter Version Management: A simple CLI to manage Flutter SDK versions.",
    "homepage": "https://fvm.app/",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://github.com/leoafarias/fvm/releases/download/4.0.4/fvm-4.0.4-windows-x64.zip",
            "hash": "e8f11729fd2567e25f663cd1e2b987f734f3ebf5ed977202a492acdfeeccefac"
        },
        "arm64": {
            "url": "https://github.com/leoafarias/fvm/releases/download/4.0.4/fvm-4.0.4-windows-arm64.zip",
            "hash": "96080660f973c18917ce0c1a4632c3a2edf1e495dd0c7b5774212ea39bd41f2f"
        }
    },
    "extract_dir": "fvm",
    "pre_install": [
        "if (!(Test-Path \"$persist_dir\\versions\")) {",
        "   New-Item \"$dir\\versions\" -ItemType Directory | Out-Null",
        "   New-Item \"$dir\\.settings\" -ItemType File | Out-Null",
        "}"
    ],
    "bin": "fvm.exe",
    "env_set": {
        "FVM_CACHE_PATH": "$dir"
    },
    "persist": [
        "versions",
        ".settings"
    ],
    "checkver": {
        "github": "https://github.com/leoafarias/fvm"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/leoafarias/fvm/releases/download/$version/fvm-$version-windows-x64.zip"
            },
            "arm64": {
                "url": "https://github.com/leoafarias/fvm/releases/download/$version/fvm-$version-windows-arm64.zip"
            }
        }
    }
}
